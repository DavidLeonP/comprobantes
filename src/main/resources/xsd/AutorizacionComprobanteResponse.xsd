<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="RespuestaAutorizacionComprobante">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="claveAccesoConsultada" type="xs:string" />
        <xs:element name="numeroComprobantes" type="xs:unsignedByte" />
        <xs:element name="autorizaciones">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="autorizacion">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="estado" type="xs:string" />
                    <xs:element minOccurs="0" name="numeroAutorizacion" type="xs:string" />
                    <xs:element name="fechaAutorizacion" type="xs:string" />
                    <xs:element name="ambiente" type="xs:string" />
                    <xs:element name="comprobante" type="xs:string" />
                    <xs:element name="mensajes">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element maxOccurs="unbounded" name="mensaje">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="identificador" type="xs:string" />
                                <xs:element name="mensaje" type="xs:string" />
                                <xs:element name="tipo" type="xs:string" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>