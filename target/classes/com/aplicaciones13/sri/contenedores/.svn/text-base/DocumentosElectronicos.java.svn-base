package sri.contenedoresDatos;


import com.utilidades.NombreDirectorio;

import sri.ArchivoXML;
import sri.Recepcion;

import sri.firma.GestorCertificados;


public class DocumentosElectronicos {

    private DeDocumentoElectronico deDocumentoElectronico = null;
    private DePasos dePasos = null;
    private DeAutorizaciones deAutorizaciones = null;

    private ArchivoXML archivoXML = new ArchivoXML();
    private GestorCertificados gestorCertificados = null;
    private NombreDirectorio nombreDirectorio = null;

    private Recepcion recepcion = new Recepcion();
    private sri.Autorizacion autorizacion = new sri.Autorizacion();

    //C:/tmp/firasGraiman/jose_augusto_vasquez_solano.p12
    private String pahtArchivoP12 = "";
    //C:/tmp/rvxml/00005-RI 001-010-000000028.xml
    private String pahtArchivoDocumentoElectronico = "";
    //C:/tmp
    private String folderTrabajo = "";
    //https://celcer.sri.gob.ec/comprobantes-electronicos-ws/RecepcionComprobantes
    private String URLRecepcionComprobante = "";
    //https://celcer.sri.gob.ec/comprobantes-electronicos-ws/AutorizacionComprobantes
    private String URLAutorizacionComprobante = "";
    //3000
    private String esperaAutorizacion;

    private String estado;
    private String proceso;

    public DocumentosElectronicos() {
        super();
        limpiar();
    }

    public void limpiar() {
        setArchivoXML(new ArchivoXML());
        setAutorizacion(new sri.Autorizacion());
        setDeDocumentoElectronico(new DeDocumentoElectronico());
        setDeAutorizaciones(new DeAutorizaciones());
        setDePasos(new DePasos());
        setEsperaAutorizacion("");
        setEstado("");
        setFolderTrabajo("");
        setGestorCertificados(null);
        setNombreDirectorio(null);
        setPahtArchivoDocumentoElectronico("");
        setPahtArchivoP12("");
        setProceso("");
        setRecepcion(null);
        setURLAutorizacionComprobante("");
        setURLRecepcionComprobante("");
    }

    public DeDocumentoElectronico getDeDocumentoElectronico() {
        return deDocumentoElectronico;
    }

    public void setDeDocumentoElectronico(DeDocumentoElectronico deDocumentoElectronico) {
        this.deDocumentoElectronico = deDocumentoElectronico;
    }

    public DePasos getDePasos() {
        return dePasos;
    }

    public void setDePasos(DePasos dePasos) {
        this.dePasos = dePasos;
    }

    public String getPahtArchivoP12() {
        return pahtArchivoP12;
    }

    public void setPahtArchivoP12(String pahtArchivoP12) {
        this.pahtArchivoP12 = pahtArchivoP12;
    }

    public String getPahtArchivoDocumentoElectronico() {
        return pahtArchivoDocumentoElectronico;
    }

    public void setPahtArchivoDocumentoElectronico(String pahtArchivoFactura) {
        this.pahtArchivoDocumentoElectronico = pahtArchivoFactura;
    }

    public String getFolderTrabajo() {
        return folderTrabajo;
    }

    public void setFolderTrabajo(String folderTrabajo) {
        this.folderTrabajo = folderTrabajo;
    }


    public String getURLRecepcionComprobante() {
        return URLRecepcionComprobante;
    }

    public void setURLRecepcionComprobante(String URLRecepcionComprobante) {
        this.URLRecepcionComprobante = URLRecepcionComprobante;
    }

    public String getURLAutorizacionComprobante() {
        return URLAutorizacionComprobante;
    }

    public void setURLAutorizacionComprobante(String URLAutorizacionComprobante) {
        this.URLAutorizacionComprobante = URLAutorizacionComprobante;
    }

    public String getEsperaAutorizacion() {
        return (esperaAutorizacion == null ||
                esperaAutorizacion.length() == 0) ? "3000" :
               esperaAutorizacion;
    }

    public void setEsperaAutorizacion(String esperaAutorizacion) {
        this.esperaAutorizacion = esperaAutorizacion;
    }

    public String getEstado() {
        return estado;
    }

    public void setEstado(String estado) {
        this.estado = estado;
    }

    public String getProceso() {
        return proceso;
    }

    public void setProceso(String proceso) {
        this.proceso = proceso;
    }

    public ArchivoXML getArchivoXML() {
        return archivoXML;
    }

    public void setArchivoXML(ArchivoXML archivoXML) {
        this.archivoXML = archivoXML;
    }

    public GestorCertificados getGestorCertificados() {
        return gestorCertificados;
    }

    public void setGestorCertificados(GestorCertificados gestorCertificados) {
        this.gestorCertificados = gestorCertificados;
    }

    public NombreDirectorio getNombreDirectorio() {
        return nombreDirectorio;
    }

    public void setNombreDirectorio(NombreDirectorio nombreDirectorio) {
        this.nombreDirectorio = nombreDirectorio;
    }

    public Recepcion getRecepcion() {
        return recepcion;
    }

    public void setRecepcion(Recepcion recepcion) {
        this.recepcion = recepcion;
    }

    public sri.Autorizacion getAutorizacion() {
        return autorizacion;
    }

    public void setAutorizacion(sri.Autorizacion autorizacion) {
        this.autorizacion = autorizacion;
    }

    public String getPathXMLOriginal() {
        return (pahtArchivoDocumentoElectronico == null) ? "" :
               pahtArchivoDocumentoElectronico;
    }

    public DeAutorizaciones getDeAutorizaciones() {
        return deAutorizaciones;
    }

    public void setDeAutorizaciones(DeAutorizaciones deAutorizaciones) {
        this.deAutorizaciones = deAutorizaciones;
    }
}
